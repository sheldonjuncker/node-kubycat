  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sheldonjuncker/node-kubycat/issues"
  },
  "homepage": "https://github.com/sheldonjuncker/node-kubycat",
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/node-notifier": "^8.0.2",
    "ava": "^4.2.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "child_process": "^1.0.2",
